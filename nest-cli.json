{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "assets": [
    {
      "include": "database/migrations/*.sql",
      "outDir": "dist"
    },
    {
      "include": "database/mock-data/*.json",
      "outDir": "dist"
    }
  ]
}
